{
  "compare": {
    "baseline_id": 1,
    "candidate_id": 2
  },
  "attributes": [
    "ANGLE2D",
    "DEPTH2D",
    "SPEED2D",
    "CUMINF2D",
    "GASMD2D",
    "GASFLAG2D",
    "GAMCUZ2D",
    "GATDUZ2D"
  ],
  "clustering": {
    "method": "connected_components",
    "k": 6,
    "max_iter": 300,
    "init": "k-means++",
    "random_seed": 42,
    "min_size": 25
  },
  "thresholds": {
    "min_cluster_area": 150.0,
    "depth_threshold": 0.005
  },
  "visualization": {
    "palette": "Blues",
    "use_gradient": true,
    "draw_mesh": false,
    "center_only": true
  },
  "outputs": {
    "make_animation": true,
    "export_geojson": true,
    "export_shapefile": false
  },
  "mcp": {
    "use_mcp": true,
    "server": "duckdb_mcp",
    "shared_context": "./runs/context/",
    "agents": [
      "ClusterOptimizer",
      "ResultsInterpreter",
      "RuleExpander",
      "Visualizer"
    ],
    "allow_agent_communication": true
  },
  "features": [
    "DEPTH2D"
  ],
  "filter": "DEPTH2D > 0.05"
}